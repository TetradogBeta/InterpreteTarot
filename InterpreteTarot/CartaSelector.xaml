<UserControl x:Class="InterpreteTarot.CartaSelector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:InterpreteTarot"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Name="grid" MouseLeftButtonUp="imgCarta_MouseLeftButtonUp">
        <Image Name="imgCarta" Margin="5,5,5,5" MouseLeftButtonUp="imgCarta_MouseLeftButtonUp"/>
    </Grid>
</UserControl>
